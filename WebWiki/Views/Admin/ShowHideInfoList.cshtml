
@{
    ViewBag.Title = "ShowHideInfoList";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<form action="" method="POST">
    <div class="table-agile-info">
        <div class="panel panel-default">
            <div class="panel-heading">
                Hide Information
            </div>
            <div class="table-responsive">
                <table class="table table-striped b-t b-light">
                    <thead>
                        <tr>
                            <th>Title Name</th>
                            <th>Category Name</th>
                            <td>Day Create</td>
                            <th>Action</th>
                        </tr>
                    </thead>
                    <tbody style="word-break: break-all;">
                        @foreach (var item in ((IEnumerable<WebWiki.MyReference.ThongTin_NguoiDung>)TempData["info"]))
                        {
                            <tr>
                                <td>@item.ChuDeTT</td>
                                <td>@item.TenDM</td>
                                <td>@item.NgayTaoTT</td>
                                <td>
                                    <a href="@Url.Action("DetailInfoAdmin",new { idIF = item.MaTT })" class="active" ui-toggle-class="">
                                        <i class="fa fa-info-circle text-success text-active"></i>&nbsp; &nbsp;
                                    </a>
                                </td>
                            </tr>
                        }

                    </tbody>
                </table>
            </div>
        </div>
    </div>
</form>

