
@{
    ViewBag.Title = "ServiceUser";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<div class="table-agile-info">
    <div class="panel panel-default">
        <div class="panel-heading">
            Service User List
        </div>
        <div class="table-responsive">
            <table class="table table-striped b-t b-light">
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>User Name</th>
                        <th>Email</th>
                    </tr>
                </thead>
                <tbody style="word-break: break-all;">
                    @foreach (var item in ((IEnumerable<WebWiki.MyReference.NguoiDichVu>)TempData["s"]))
                    {
                        <tr>
                            <td>@item.TenNguoiDV</td>
                            <td>@item.MailNguoiDV</td>
                            <td>@(item.KichHoatNguoiDV == true ? "Yes" : "No")</td>
                        </tr>
                    }

                </tbody>
            </table>
        </div>
    </div>
</div>

